.PHONY: clean All

All:
	@echo "----------Building project:[ basic__IO - Debug ]----------"
	@cd "basic__IO" && "$(MAKE)" -f  "basic__IO.mk" && "$(MAKE)" -f  "basic__IO.mk" PostBuild
clean:
	@echo "----------Cleaning project:[ basic__IO - Debug ]----------"
	@cd "basic__IO" && "$(MAKE)" -f  "basic__IO.mk" clean
